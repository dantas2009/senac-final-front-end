{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "compra-inteligente": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/compra-inteligente",
            "baseHref": "/",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/deeppurple-amber.css",
              "src/assets/css/app.min.css",
              "src/assets/css/style.css",
              "src/assets/css/components.css",
              "node_modules/bootstrap-social/bootstrap-social.css",
              "src/assets/css/bootstrap-datetimerpicker.css",
              "src/assets/bundles/bootstrap-daterangepicker/daterangepicker.css",
              "src/assets/bundles/datatables/datatables.min.css",
              "src/assets/bundles/datatables/DataTables-1.10.16/css/dataTables.bootstrap4.min.css"

            ],
            "scripts": [
              "src/assets/js/bootstrap-datetimerpicker.js",
              "src/assets/bundles/bootstrap-daterangepicker/daterangepicker.js",
              "src/assets/bundles/datatables/datatables.min.js",
              "src/assets/bundles/datatables/DataTables-1.10.16/js/dataTables.bootstrap4.min.js",
              "src/assets/bundles/jquery-ui/jquery-ui.min.js"
            ],
            "server": "src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "20mb",
                  "maximumError": "50mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "proxyConfig": "proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "compra-inteligente:build:production"
            },
            "development": {
              "buildTarget": "compra-inteligente:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "compra-inteligente:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/deeppurple-amber.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
